<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/image17"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:src="@drawable/a"
            android:layout_alignParentLeft="true"
            android:paddingLeft="10dp"
            android:layout_marginTop="20dp"/>
        <ImageView
            android:id="@+id/image18"
            android:layout_width="100dp"
            android:layout_height="65dp"
            android:src="@drawable/u23"
            android:layout_toRightOf="@+id/image17"
            android:layout_marginTop="20dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/image15"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:src="@drawable/a"
            android:layout_alignParentRight="true"
            android:paddingRight="10dp"
            android:layout_marginTop="20dp"/>
        <ImageView
            android:id="@+id/image16"
            android:layout_width="100dp"
            android:layout_height="65dp"
            android:src="@drawable/u21"
            android:layout_toLeftOf="@+id/image15"
            android:layout_marginTop="20dp"/>
    </RelativeLayout>
</LinearLayout>
